{
  "env": {
    "es2020": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "eslint-config-leapfrog",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "camelcase": [
      "error",
      {
        "properties": "never"
      }
    ]
  }
}
